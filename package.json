{
  "name": "ubahnchen",
  "version": "0.0.1",
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean && del-cli \"**/.turbo\" -d",
    "purge": "yarn clean && del-cli \"**/node_modules\" node_modules -d",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint --parallel --",
    "lintall": "eslint . --quiet --fix --ext .js,.ts,.jsx,.tsx,.html,.vue,.md,.json,.sql,.mjs,.cjs",
    "ncu": "npx npm-check-updates --workspaces --upgrade --root",
    "depcheck": "lerna exec --no-bail --stream -- depcheck",
    "sort-packages": "npx sort-package-json \"package.json\" \"**/*/package.json\"",
    "turbo-test": "turbo run test",
    "test": "vitest"
  },
  "devDependencies": {
    "@babel/core": "^7",
    "@babel/preset-env": "^7",
    "@babel/preset-react": "^7",
    "@babel/preset-typescript": "^7",
    "@types/jsdom": "^21",
    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "@ubahnchen/cli": "*",
    "@vitest/coverage-v8": "^2",
    "babel-plugin-transform-import-meta": "^2",
    "babel-preset-typescript": "7.0.0-alpha.19",
    "del-cli": "^5",
    "depcheck": "^1",
    "eslint": "^8",
    "eslint-config-next": "^14",
    "esm": "^3",
    "jsdom": "^25",
    "jsonc-eslint-parser": "^2",
    "lerna": "^8",
    "npm-check-updates": "^17",
    "prettier": "^3",
    "prettier-plugin-sql-cst": "^0",
    "prettier-plugin-tailwindcss": "^0",
    "tsup": "^8",
    "turbo": "^2",
    "typescript": "^5",
    "vitest": "^2",
    "vitest-canvas-mock": "^0"
  },
  "private": true,
  "license": "MIT",
  "type": "module",
  "workspaces": {
    "packages": [
      "apps/*",
      "libs/*",
      "packages/*",
      "tests/*"
    ]
  },
  "packageManager": "yarn@1.22.22",
  "engines": {
    "node": ">=10"
  },
  "volta": {
    "node": "20.12.1"
  }
}
