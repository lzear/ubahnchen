{
  "name": "ubahnchen",
  "version": "0.0.1",
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean && del-cli \"**/.turbo\" -d",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint --",
    "ncu": "npx npm-check-updates --workspaces --upgrade --root",
    "sort-packages": "npx sort-package-json \"package.json\" \"**/*/package.json\"",
    "test": "node --experimental-vm-modules --no-warnings node_modules/.bin/jest",
    "turbo-test": "turbo run test"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@types/jest": "^29.5.1",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "babel-jest": "^29.5.0",
    "babel-preset-typescript": "^7.0.0-alpha.19",
    "del-cli": "^5.0.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.40.0",
    "eslint-config-next": "13.4.1",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-ubahnchen": "*",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "esm": "^3.2.25",
    "jest": "^29.5.0",
    "jsonc-eslint-parser": "^2.2.0",
    "npm-check-updates": "^16.10.9",
    "prettier": "^2.8.8",
    "prettier-plugin-sql-cst": "^0.8.2",
    "tmp": "^0.2.1",
    "ts-jest": "^29.1.0",
    "turbo": "^1.9.3",
    "typescript": "^5.0.4"
  },
  "private": true,
  "license": "MIT",
  "type": "module",
  "workspaces": {
    "packages": [
      "apps/*",
      "libs/*",
      "packages/*",
      "tests/*"
    ]
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=10"
  },
  "volta": {
    "node": "18.14.0"
  }
}
