{
  "name": "ubahnchen",
  "version": "0.0.1",
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean && del-cli \"**/.turbo\" -d",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint --",
    "lintall": "eslint . --quiet --fix --ext .js,.ts,.jsx,.tsx,.html,.vue,.md,.json,.sql,.mjs,.cjs",
    "ncu": "npx npm-check-updates --workspaces --upgrade --root",
    "depcheck": "lerna exec --no-bail --stream -- depcheck",
    "sort-packages": "npx sort-package-json \"package.json\" \"**/*/package.json\"",
    "test": "node --experimental-vm-modules --no-warnings node_modules/.bin/jest",
    "test-coverage": "yarn test --coverage",
    "turbo-test": "turbo run test"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@jest/globals": "^29.5.0",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "babel-jest": "^29.5.0",
    "babel-plugin-transform-import-meta": "^2.2.0",
    "babel-preset-typescript": "^7.0.0-alpha.19",
    "del-cli": "^5.0.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.42.0",
    "eslint-config-next": "^13.4.6",
    "esm": "^3.2.25",
    "jest": "^29.5.0",
    "jsonc-eslint-parser": "^2.3.0",
    "lerna": "^7.0.2",
    "npm-check-updates": "^16.10.12",
    "prettier": "^2.8.8",
    "prettier-plugin-sql-cst": "^0.8.2",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "ts-jest": "^29.1.0",
    "tsup": "^7.0.0",
    "turbo": "^1.10.3",
    "typescript": "^5.1.3"
  },
  "private": true,
  "license": "MIT",
  "type": "module",
  "workspaces": {
    "packages": [
      "apps/*",
      "cities/*",
      "libs/*",
      "packages/*",
      "submodules/*",
      "tests/*"
    ]
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=10"
  },
  "volta": {
    "node": "18.14.0"
  }
}
