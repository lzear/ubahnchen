    "typedoc-plugin-keywords": "^1.6.0",
    "typedoc-plugin-markdown": "^3.17.1",
    "typedoc-theme-hierarchy": "^4.1.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0"
